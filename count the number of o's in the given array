class Main
{
    public static void main(String args[])
    {
        int arr[]={1,1,1,1,1,1,1,1,1};
        int count=0;
        int n=arr.length;
        for(int i=0;i<=n-1;i++)
        {
            if(arr[i]==0)
            {
                count++;
            }
            else
            {
                break;
            }
        }
        System.out.println(count);
    }
}
